#include<iostream>
using namespace std;
int main() {
    int a= 123;
    cout << a << endl;
    
    char b = 'a';
    cout << b << endl;
    
    bool b1= true;
    cout << b1 << endl;
    
    float c = 123.456;
    cout << c << endl;
    
    double d = 123.456;
    cout << d << endl;
    
    int size = sizeof(a); 
    cout << size << endl;
    


}
